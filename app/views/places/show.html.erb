<h1><%= @place["name"] %></h1>

<ul>
    <% for post in @posts %>
        <li>
            <%= post["title"] %>
            <%= post["description"] %>
            <%= post["posted_on"] %>
        </li>
    <% end %>
</ul>

<p>
    <a href="/places"> Back to all places </a>
</p>

<a href="/posts/new?place_id= <%= @place["id"] %>">New Post for <%= @place["name"]%> </a>
